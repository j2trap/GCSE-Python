num1 = int(input("Give me an number."))
num2 = int(input("Give me another number."))

if num1 > num2:
    print(num2, num1)

else:
    print(num1, num2)

2. 
num1 = int(input("Enter a number below 20"))

if num1 >= 20:
    print("too high")

else:
    print("thank you")
3. 
num1 = int(input("Enter a number between 10 and 20"))

if num1 > 10 and num1 < 20:
    print("thank you")

else:
    print("incorrect answer")
4. 
favcolor = str(input("what is your favourite colour>"))

if favcolor == "red" or favcolor =="RED" or favcolor == "Red":
    print("i like red too")

else:
    print("I don't like", favcolor,",",favcolor ,"is for losers")
5. 
rain = str(input("Is it raining")).lower()

if rain == "yes":
    wind = str(input("Is it windy?")).lower()
    if wind == "yes":
        print("It is too windy for an umbrella")
    else:
        print("take an umbrella")
else:
    print("Enjoy your day")
6. 
age = int(input("How old are you"))
if age >= 18:
    print("you can vote.")
elif age == 17:
    print("you can learn to drive.")
elif age == 16:
    print("you can buy a lottery ticket.")
else:
    print(" you can go trick or treating.")
