for i in range (1,67):
 print("thomas is really smart")   #this is a string
 print("4")   #this is an intger
 print("4.5")   #this is a real/float
 print("$")   #this is a character
 print(True)   #this is a boolean

#data types
#CRIBS
#C - character e.g "a"
#R - real/float e.g 4.66
#I - integer e.g. 222
#B - boolean e.g. True
#S - sting e.g. "hello"
